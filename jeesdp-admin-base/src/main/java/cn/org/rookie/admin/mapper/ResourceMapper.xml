<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="cn.org.rookie.admin.mapper.ResourceMapper">

    <select id="selectConsumerResources" resultType="cn.org.rookie.common.base.entity.Resource">
        select * from admin_user_resource_all
        where user_id = #{userId}
    </select>

    <select id="getMaxIdByPid" parameterType="String" resultType="String">
        select max(id) from admin_resource where pid = #{pid}
    </select>
</mapper>
	 
